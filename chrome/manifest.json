{
  "manifest_version": 3,
  "name": "RWTH Sign-In Extender",
  "version": "1.0",
  "description": "All sign in cookie lenghts are prologed to 2 days.",
  "content_scripts": [
    {
      "matches": ["*://*.rwth-aachen.de/*"],
      "js": ["js.js"]
    }
  ],

  "background": {
    "service_worker": ["background.js"]
  },
  "permissions": ["cookies", "*://*.rwth-aachen.de/*"]
}
